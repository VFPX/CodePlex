	SELECT (THIS.oWeb.cWSalias)
	LOCATE FOR UPPER(ALLTRIM(UniqueID)) == UPPER(ALLTRIM(lcWS)) AND;
		UPPER(Type) = "C"
	IF !FOUND()
		RETURN
	ENDIF
ENDIF

