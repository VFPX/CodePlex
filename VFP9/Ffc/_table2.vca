	IF TYPE("oForm ")#"U" AND;
	  UPPER(oForm.NAME)==UPPER(THIS.Parent.cClass);
	  AND oForm.Visible
	  	WITH THIS.Parent
			.nLastTop = oForm.Top
			.nLastLeft = oForm.Left
			.lDocked = oForm.Docked
		ENDWITH
		RETURN
	ENDIF
ENDFOR

THIS.Interval = 0
THIS.Parent.oToolbar.Top = THIS.Parent.nLastTop
THIS.Parent.oToolbar.Left = THIS.Parent.nLastLeft
