PROCEDURE SetFocus
local oControl
for each oControl in this.Controls
	if type("oControl.TabIndex") = "N"
		if oControl.TabIndex = 1
			if pemstatus( m.oControl,"setFocus",5)
				oControl.SetFocus()
				nodefault
			endif
			exit
		endif
	endif
endfor

ENDPROC
