			OTHERWISE
				This.SetStatus(xfcGdipCreatePen1( EVL(m.toColor,ARGB_Black), m.tnWidth, UnitWorld, @lhPen))
			ENDCASE
			
			This.Handle = m.lhPen
		CATCH TO loExc
