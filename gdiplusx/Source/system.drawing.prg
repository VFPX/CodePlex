				m.lqPoints = m.loPoint.ToVarBinary(@taoPts, @liCount)
				This.SetStatus(xfcGdipTransformPointsI(This.Handle, m.tiDestSpace, m.tiSrcSpace, @lqPoints, m.liCount))
