				m.loPoint = m.tiX
				m.loPoint.GetExtent(@tnX, @tnY)
			ENDCASE
			
			m.llValue = (m.tiX>=This.X ;
						AND m.tiY>=This.Y ;
						AND m.tiX+m.tiWidth<=This.X+This.Width ;
						AND m.tiY+m.tiHeight<=This.Y+This.Height)
			
