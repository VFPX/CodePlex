				m.lnBytesPerScan = m.lnBytesPerScan + 4 - MOD(m.lnBytesPerScan, 4)
			ENDIF
		
			m.lcBIHdr = ;
				BINTOC(BHDR_SIZE ,"4RS") + ;
				BINTOC(m.lnWidth,"4RS") + ;
				BINTOC(m.lnHeight, "4RS") + ;
				0h0101 + ;
				(CHR(MOD(m.lnBitsPerPixel,256))+ CHR(INT(m.lnBitsPerPixel/256))) + ;
