		[<memberdata name="cananimate" type="method" display="CanAnimate"/>]+;
		[<memberdata name="stopanimate" type="method" display="StopAnimate"/>]+;
		[<memberdata name="updateframes" type="method" display="UpdateFrames"/>]+;
		[</VFPData>]		
	#ENDIF
ENDDEFINE
#ENDIF
