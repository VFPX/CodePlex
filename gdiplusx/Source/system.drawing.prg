			CASE VARTYPE(m.tiX)+VARTYPE(m.tiY)+VARTYPE(m.tiWidth)+VARTYPE(m.tiHeight) == "NNNN"
			
			CASE VARTYPE(m.tiX) = "Q" OR (VARTYPE(m.tiX) = "C" AND LEN(m.tnX) = 16)
