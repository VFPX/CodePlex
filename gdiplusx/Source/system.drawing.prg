				m.tiWidth = CTOBIN(SUBSTR(lqStruct,9,4),"4rs")
				m.tiHeight = CTOBIN(SUBSTR(lqStruct,13,4),"4rs")
			
			OTHERWISE
				STORE 0 TO m.tiX, m.tiY, m.tiWidth, m.tiHeight
			ENDCASE
			
			This.X      = (m.tiX)
