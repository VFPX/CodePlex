PROCEDURE multiply
*********************************************************************
** Method: Multiply
**
** Returns a new ColorMatrix resulting of the multiplication
** of two color matrices
**
** History:
** 2006/06/28: CBoyd - Coded
*********************************************************************
LPARAMETERS toColorMatrixOne, ;
			toColorMatrixTwo
LOCAL loCombinedMatrix, lnCounter, lnColumn, lnRow, lnCombinedValue
m.loCombinedMatrix = NULL

TRY
   IF NOT(VARTYPE(m.toColorMatrixOne)+VARTYPE(m.toColorMatrixOne)=="OO")
      *! ToDo: Error?
      EXIT
   ENDIF

   m.loCombinedMatrix = This.New()
   FOR m.lnCounter = 1 TO 5
      FOR m.lnRow = 1 TO 5
         m.lnCombinedValue = 0
         FOR m.lnColumn = 1 TO 5
             m.lnCombinedValue = m.lnCombinedValue + ;
                (m.toColorMatrixTwo.Item(m.lnRow, m.lnColumn) * ;
                m.toColorMatrixOne.Item(m.lnColumn, m.lnCounter))
         ENDFOR
         m.loCombinedMatrix.Item(m.lnRow, m.lnCounter) = m.lnCombinedValue
      ENDFOR
   ENDFOR

CATCH TO loExc
   THROW m.loExc

ENDTRY
RETURN m.loCombinedMatrix
ENDPROC
