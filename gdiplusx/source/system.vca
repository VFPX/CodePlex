PROCEDURE parse
LPARAMETERS toEnum, tcMemberName
LOCAL loExc AS EXCEPTION
TRY
	m.lvReturn = GETPEM(m.toEnum, tcMemberName)
CATCH TO loExc
	THROW m.loExc
ENDTRY
RETURN m.lvReturn

ENDPROC
