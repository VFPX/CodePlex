[CLASS] imgcanvas
[CLASSLOC] ..\source\gdiplusx.vcx
[BASECLASS] image
[OBJNAME] Imgcanvas2
[PARENT] Form1
[START PROPERTIES]
Anchor = 15
Height = 60
Left = 240
