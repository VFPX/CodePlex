[BASECLASS] image
[OBJNAME] Imgcanvas1
[PARENT] shapes
[START PROPERTIES]
Anchor = 15
Height = 252
Left = 7
Top = 7
Width = 377
Name = "Imgcanvas1"
[END PROPERTIES]
[START METHODS]
PROCEDURE beforedraw
** This sample was coverted from a .NET sample at Bob Powell's web site
** http://www.bobpowell.net/strokefill.htm


WITH _SCREEN.SYSTEM.drawing
	This.Clear()
	
	** Lines ************************************
	**   Literal:
	THIS.oGfx.DrawLine(.Pens.Red, 10, 5, 210, 15)
	**   Using Points
	p1 = .Point.New(10, 8)
	p2 = .Point.New(210, 18)
	THIS.oGfx.DrawLine(.Pens.Blue, p1, p2)
	
	
	** Sine Wave ********************************
	DIMENSION pts(20)
	angle = 0
	FOR x = 1 TO 20
		pts(x) = .PointF.New((x-1)*10+50, 70+40*SIN(angle))
		angle = angle + PI()/10
	NEXT
	THIS.oGfx.DrawLines(.Pens.Aqua, @pts)
	LOCAL p
	FOR EACH p IN pts
		THIS.oGfx.FillEllipse(.Brushes.Violet, ;
			.RectangleF.NEW(p.x - 3, p.Y - 3, 6.0, 6.0))
	NEXT

	** Polygon **********************************
	DIMENSION POLY(5)
	POLY[1] = .Point.New(200,150)
	POLY[2] = .Point.New(350,200)
	POLY[3] = .Point.New(350,150)
	POLY[4] = .Point.New(200,200)
	POLY[5] = .Point.New(200,150)
	THIS.oGfx.FillPolygon(.Brushes.PeachPuff, @POLY)
	PEN = .Pen.New(.Color.Olive, 4)
	THIS.oGfx.DrawPolygon(PEN, @POLY)
	PEN.Dispose()
	
	** Rectangles *******************************
	DIMENSION rcs(10)
	FOR x = 1 TO 10
		rcs(x) = .Rectangle.New(80 - x * 5, 110 + x * 12, x * 10, 12)
	NEXT x
	THIS.oGfx.FillRectangles(.Brushes.RoyalBlue, @rcs)
	THIS.oGfx.DrawRectangles(.Pens.YellowGreen, @rcs)
ENDWITH

ENDPROC
[END METHODS]

