PROCEDURE beforedraw


LOCAL loRect AS xfcRectangle
LOCAL loBrush AS xfcLinearGradientBrush
LOCAL loBlend AS xfcBlend


WITH _SCREEN.System.Drawing
	loBrush = .Drawing2D.LinearGradientBrush.New(This.Rectangle, .Color.Red, .Color.Blue, ;
					.Drawing2D.LineargradientMode.ForwardDiagonal)
	loBlend = .Drawing2D.Blend.New(5)
	loBlend.Factors =   _SCREEN.System.Single.NewArray(0.0, 0.5, 0.1, 0.5, 0.0)
	loBlend.Positions = _SCREEN.System.Single.NewArray(0.0, 0.2, 0.5, 0.7, 1.0)
	
	loColorBlend = .Drawing2D.ColorBlend.New(5)
	loColorBlend.Colors = .Color.NewArray(.Color.Red, .Color.Yellow, .Color.Green, .Color.Blue, .Color.Violet)
	loColorBlend.Positions = _SCREEN.System.Single.NewArray(0.0, 0.2, 0.5, 0.7, 1.0)

	loBrush.Blend = m.loBlend
	loBrush.InterpolationColors = loColorBlend
	
	x=loBrush

	This.oGfx.FillRectangle(loBrush, This.Rectangle)
ENDWITH



